{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:3000",
    "distDir": "../public"
  },
  "package": {
    "productName": "outliner",
    "version": "0.0.1-alpha-4"
  },
  "tauri": {
    "allowlist": {
      "http": {
        "scope": [
          "http://localhost:9090"
        ]
      },
      "shell": {
        "open": true
      },
      "fs": {
        "scope": [
          "**",
          "$APPLOCALDATA/outliner",
          "$APPLOCALDATA/outliner/*",
          "$APPLOCALDATA/outliner/contentNodes/*",
          "$APPLOCALDATA/outliner/images/*",
          "$HOME/**/*"
        ],
        "all": true
      },
      "path": {
        "all": true
      },
      "dialog": {
        "open": true
      },
      "window": {
        "setTitle": true
      },
      "protocol": {
        "asset": true,
        "assetScope": [
          "$APPLOCALDATA/outliner/images/**"
        ]
      },
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "Productivity",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.outliner.xangelo",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": "default-src 'self'; img-src 'self' asset: https://asset.localhost"
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "outliner",
        "width": 800
      }
    ]
  }
}
