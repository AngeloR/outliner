    "0da3090a-9a6f-4b3f-9607-0e97bd4318f5": {
      "id": "0da3090a-9a6f-4b3f-9607-0e97bd4318f5",
      "created": 1674504442196,
      "type": "text",
      "content": "Current"
    },
    "d69efd32-fbf5-4b80-8a36-af1cb2af7b80": {
      "id": "d69efd32-fbf5-4b80-8a36-af1cb2af7b80",
      "created": 1674504453605,
      "type": "text",
      "content": "- [ ] Search!"
    },
    "b5a55548-f65d-496a-8bea-6b6ba8296e21": {
      "id": "b5a55548-f65d-496a-8bea-6b6ba8296e21",
      "created": 1674504458424,
      "type": "text",
      "content": "This is the most important one. The ability to fuzzy search across the entirety of the document and jump to any node possible. When I was working with Fargo + Little Outliner, I tried building extensions that would allow me to do this - but they never quite worked as I envisioned. I'm hoping to provide a full fuzzy-text search system<br>"
    },
    "315c3870-a6c2-49b6-89b1-8c9e0a950b4c": {
      "id": "315c3870-a6c2-49b6-89b1-8c9e0a950b4c",
      "created": 1674504522443,
      "type": "text",
      "content": "---"
    },
